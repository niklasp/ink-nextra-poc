{
  "name": "ink-nextra",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build",
    "postbuild": "npm run create-llm-file",
    "start": "next start",
    "lint": "next lint",
    "create-llm-file": "npx repomix --style markdown -o llms.txt -i \"*.svg,static/**,node_modules/**,.next/**,public/**,build/**,out/**\"",
    "create-llm-file-remote": "npx repomix --remote https://github.com/use-ink/ink-docs --style markdown -o llms.txt -i \"*.svg,static/**\""
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^2.0.12",
    "@tailwindcss/postcss": "^4.0.6",
    "clsx": "^2.1.1",
    "lottie-react": "^2.4.1",
    "next": "^15.1.7",
    "nextra": "^4.2.12",
    "nextra-theme-docs": "^4.2.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-spring": "^9.7.5",
    "sass": "^1.85.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "22.13.4",
    "@types/react": "19.0.10",
    "@types/react-google-recaptcha": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.2",
    "tailwindcss": "^4.0.6"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
